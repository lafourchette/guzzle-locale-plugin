{
    "name": "lafourchette/guzzle-locale-plugin",
    "description": "Guzzle plugin to set locale on query",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.lafourchette.com"
        }
    ],
    "require": {
        "guzzle/guzzle": "~3.7"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.5"
    },
    "autoload": {
        "psr-4": { "Guzzle\\Plugin\\Locale\\": "src" }
    },
    "config": {
        "bin-dir": "bin"
    }
}
